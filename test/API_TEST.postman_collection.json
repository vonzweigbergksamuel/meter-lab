										"exec": [
											"// Function to generate a random string of specified length\r",
											"function getRandomString(length) {\r",
											"    let chars = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ';\r",
											"    let result = '';\r",
											"    for (let i = 0; i < length; i++) {\r",
											"        result += chars.charAt(Math.floor(Math.random() * chars.length));\r",
											"    }\r",
											"    return result;\r",
											"}\r",
											"\r",
											"// Function to generate a random email\r",
											"function getRandomEmail() {\r",
											"    let randomString = getRandomString(5);\r",
											"    return `${randomString}${Math.floor(Math.random() * 1000)}@student.lnu.se`;\r",
											"}\r",
											"\r",
											"// Function to generate a password (minimum 10 characters, including numbers & special characters)\r",
											"function getRandomPassword() {\r",
											"    let numbers = \"0123456789\";\r",
											"    let basePassword = getRandomString(6) + \r",
											"                       numbers.charAt(Math.floor(Math.random() * numbers.length)) + \r",
											"                       getRandomString(3); // Ensures length of at least 10\r",
											"    return basePassword;\r",
											"}\r",
											"\r",
											"pm.collectionVariables.set(\"email\", getRandomEmail());\r",
											"pm.collectionVariables.set(\"name\", getRandomString(6));\r",
											"pm.collectionVariables.set(\"password\", getRandomPassword());"
