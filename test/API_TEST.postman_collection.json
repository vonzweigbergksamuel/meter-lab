			"item": [
				{
					"name": "Device",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Check if the response status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response body has an array property named 'devices'\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property(\"devices\");",
									"    pm.expect(jsonData.devices).to.be.an(\"array\");",
									"});",
									"",
									"pm.test(\"Each device has string properties 'device_id' and 'device_status'\", function () {",
									"    // Expect each object in the array to have \"device_id\" and \"device_status\" as strings",
									"    const jsonData = pm.response.json();",
									"    jsonData.devices.forEach(function (device) {",
									"        pm.expect(device).to.be.an(\"object\");",
									"        pm.expect(device).to.have.property(\"device_id\").that.is.a(\"string\");",
									"        pm.expect(device).to.have.property(\"device_status\").that.is.a(\"string\");",
									"    });",
									"});",
									"",
									"const response = pm.response.json()",
									"if (response.devices.length > 0) {",
									"    const device = response.devices[0]",
									"    console.log(device.device_id)",
									"    pm.collectionVariables.set(\"device_id\", device.device_id) // Set the first device_id to a collection variable",
									"    pm.collectionVariables.set(\"device_status\", device.device_status) // Set the first device_status to a collection variable",
									"}",
									""
